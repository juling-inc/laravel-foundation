{
    "name": "juling/laravel-foundation",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.2",
        "alibabacloud/aliyun-log-php-sdk": "^0.6.3",
        "aliyuncs/oss-sdk-php": "^2.7",
        "endroid/qr-code": "^5.0",
        "elasticsearch/elasticsearch": "^8.10",
        "firebase/php-jwt": "^6.10",
        "intervention/image": "^3.4",
        "laravel/framework": "^11.0",
        "laravel/octane": "^2.3",
        "laravel/sanctum": "^4.0",
        "overtrue/easy-sms": "^2.6",
        "overtrue/pinyin": "^5.3",
        "overtrue/socialite": "^4.9",
        "phpoffice/phpspreadsheet": "^2.0",
        "qcloud/cos-sdk-v5": "^2.6",
        "qiniu/php-sdk": "^7.12",
        "w7corp/easywechat": "^6.15",
        "workerman/crontab": "^1.0",
        "workerman/gateway-worker": "^3.1",
        "yansongda/pay": "^3.6"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Juling\\Foundation\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juling\\Foundation\\FoundationServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
